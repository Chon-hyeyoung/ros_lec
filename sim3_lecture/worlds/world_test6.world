<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">
    <include>
      <uri>model://sun</uri>
    </include>
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <include>
      <static>true</static>
      <uri>model://surface1</uri>
      <pose>0 0 0 0 0 0</pose>
    </include>
  </world>
</sdf>

